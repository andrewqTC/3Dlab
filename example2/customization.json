{
    "path": "https://raw.githubusercontent.com/andrewqTC/3Dlab/main/example2/",
    "options": [
      {
        "name": "features",
        "type": "string",
        "choices": ["eyes", "carrot"]
      },
      {
        "name": "shape",
        "type": "string",
        "choices": ["cube", "sphere", "cone"]
      },
      {
        "name": "model",
        "type": "string",
        "choices": ["snowman", "shapes"]
      }
    ],
    "models": [
      {
        "name": "snowman",
        "link": "prototypeSnowman.babylon",
        "loadIf": {
          "condition": "attribute",
          "isEqual": [
            {
              "option": "model",
              "choice": "snowman"
            }
          ],
          "isNotEqual": []
        },
        "disposeIf": {
          "condition": "attribute",
          "isEqual": [],
          "isNotEqual": [
            {
              "option": "model",
              "choice": "snowman"
            }
          ]
        }
      },
      {
        "name": "shapes",
        "link": "blenderShapes.babylon",
        "loadIf": {
          "condition": "attribute",
          "isEqual": [
            {
              "option": "model",
              "choice": "shapes"
            }
          ],
          "isNotEqual": []
        },
        "disposeIf": {
          "condition": "attribute",
          "isEqual": [],
          "isNotEqual": [
            {
              "option": "model",
              "choice": "shapes"
            }
          ]
        }
      }
    ],
    "tags": [
      {
        "type": "visibilitySwitch",
        "option": "shape",
        "case": {
          "cube": "box",
          "sphere": "sphere",
          "cone": "cone"
        }
      },
      {
        "type": "visibilitySwitch",
        "option": "features",
        "case": {
          "carrot": "nose",
          "eyes": "eyes"
        }
      }
    ]
  }
